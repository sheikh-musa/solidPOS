{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"jsxImportSource": "solid-js",
		"jsx": "preserve",
		"strict": true,
		"types": ["solid-start/env"],
		"baseUrl": "./",
		"paths": {
			"~/*": ["./src/*"]
		},
		"lib": ["esnext", "dom"],
		"sourceMap": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true
	}
}
